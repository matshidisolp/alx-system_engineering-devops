!/usr/bin/env bash
# Display number 1-20 with special display after 4,9 and 17

counter=1

while [ $counter -le 20 ]; do
    case $counter in
        4)
            echo "bad luck from China"
	    ;;
        9)
            echo "bad luck from Japan"
	    ;;
        17)
            echo "bad luck from Italy"
	    ;;
        *)
            echo $counter
	    ;;
    esac

    ((counter++))
done
